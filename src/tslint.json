{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            false,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            false,
            "element",
            "app",
            "kebab-case"
        ],
        "no-inferrable-types": [false],
        "prefer-const": [false]
    }
}
